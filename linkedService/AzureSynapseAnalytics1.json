{
	"name": "AzureSynapseAnalytics1",
	"type": "Microsoft.Synapse/workspaces/linkedservices",
	"properties": {
		"parameters": {
			"db": {
				"type": "string"
			},
			"user": {
				"type": "string"
			},
			"pw": {
				"type": "string"
			},
			"server": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureSqlDW",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().server};Initial Catalog=@{linkedService().db};User ID=@{linkedService().user};Password=@{linkedService().pw}"
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	}
}